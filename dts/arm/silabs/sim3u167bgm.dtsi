#include <mem.h>
#include <silabs/sim3u.dtsi>

/ {
	sram0: memory@20000000 {
		reg = <0x20000000 DT_SIZE_K(32)>;
	};

	soc {
		compatible = "silabs,sim3u167bgm", "silabs,sim3u", "silabs,sim3", "simple-bus";

		flash-controller@4002e000 {
			flash0: flash@0 {
				reg = <0 DT_SIZE_K(256)>;
			};
		};
	};
};
